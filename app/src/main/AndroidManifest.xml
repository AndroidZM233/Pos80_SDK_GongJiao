<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.spd.bus">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.CHANGE_CONFIGURATION"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />

    <!-- 开机启动 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/launchers"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <meta-data
            android:name="companyId"
            android:value="b8254759-295e-43c7-b4d1-0acb1f2a0c33" />
        <activity
            android:name=".spdata.test"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <receiver android:name=".spdata.BootBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />

                <category android:name="android.intent.category.HOME" />
            </intent-filter>
        </receiver>

        <activity android:name=".readcard.ReadCardManager" />
        <activity android:name=".readcard.CardReader" />
        <activity android:name=".system.SystemActivity" />
        <activity
            android:name=".pin.Pin"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".readcard.CardReaderNew" />
        <activity android:name=".data.DataEnDecrypt" />
        <activity android:name=".data.Mac" />
        <activity android:name=".key.EraseKeys" />
        <activity android:name=".key.KeyManager" />
        <activity android:name=".util.keyrandom" />
        <activity android:name=".key.KeyEx" />
        <activity android:name=".print.BTPrinting" />
        <activity android:name=".dukpt.DUKPT" />
        <activity android:name=".print.Printing" />
        <activity android:name=".print.PrinterManager" />
        <activity android:name=".readcard.IDCardMana" />
        <activity android:name=".sp.Spupdate" />
        <activity android:name=".dock.DockTest" />
        <activity android:name=".dock.Docksendpicture" />
        <activity android:name=".dock.Dockupdate" />
        <activity android:name=".dock.Docktesttipsshow" />
        <activity android:name=".key.CheckkeyExist" />
        <activity android:name=".emv.EmvActivity" />
        <activity android:name=".emv.InputMoneyActivity" />
        <activity android:name=".emv.PayActivity" />

        <receiver
            android:name=".MyReceiver"
            android:exported="true"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.wpos.cardinfo.callback" />
            </intent-filter>
        </receiver>

        <activity android:name=".MainActivity" />
        <activity android:name=".spdata.oldPsamIcActivityold" />
    </application>

</manifest>